{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "dev:safe": "NODE_OPTIONS='--max-old-space-size=4096' nuxt dev",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' nuxt dev --no-fork-ts-checker",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "clean": "rm -rf .nuxt node_modules/.vite node_modules/.cache",
    "analyze": "nuxt build --analyze"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@nuxt/eslint": "^1.5.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.2",
    "@pinia/nuxt": "^0.11.2",
    "@vuepic/vue-datepicker": "^11.0.2",
    "eslint": "^9.30.1",
    "nuxt": "^3.17.6",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "vue-tsc": "^2.2.12"
  }
}
